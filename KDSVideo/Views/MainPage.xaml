<Page
    x:Class="KDSVideo.Views.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}"
    DataContext="{Binding MainViewModel, Source={StaticResource Locator}}">

    <Grid>
        <Button Content="Button" Margin="130,212,0,0" VerticalAlignment="Top" TabIndex="1"/>
        <Button Margin="130,276,0,0" VerticalAlignment="Top" Command="{Binding NavigateCommand}" Style="{StaticResource NavigationBackButtonNormalStyle}" TabIndex="0">
            <Button.KeyboardAccelerators>
                <!--<KeyboardAccelerator  Key="GoBack" Modifiers="None"/>-->
                <KeyboardAccelerator  Key="Left" Modifiers="Menu"/>
            </Button.KeyboardAccelerators>
        </Button>
    </Grid>
</Page>
